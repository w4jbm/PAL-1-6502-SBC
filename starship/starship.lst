.2034	20 33 32		JSR TGLOAD

			; Set Shutdown Mode register to Normal Operation

.2037	a9 fc			LDA #%11111100
.2039	20 07 32		JSR SNDBYT
.203c	a9 01			LDA #%00000001
.203e	20 07 32		JSR SNDBYT
.2041	a9 fc			LDA #%11111100
.2043	20 07 32		JSR SNDBYT
.2046	a9 01			LDA #%00000001
